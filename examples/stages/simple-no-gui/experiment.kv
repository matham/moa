<NiceDelay@Delay>:
    on_stage_start: print('{} stage started'.format(self.knsname))
    on_trial_start: print('{} trial {} started'.format(self.knsname, self.count))
    on_trial_end: print('{} trial {} finished'.format(self.knsname, self.count))
    on_stage_end: print('{} stage finished. Elapsed time={}'.format(self.knsname, self.elapsed_time))

<RootStage>:
    NiceDelay:
        knsname: 'stage_1'
        delay: 10.1
    MoaStage:
        order: 'parallel'
        NiceDelay:
            knsname: 'stage_2_1'
            delay: 2.2
        NiceDelay:
            knsname: 'stage_2_2'
            delay: 4.5
    NiceDelay:
        knsname: 'stage_3'
        delay: 3.14
